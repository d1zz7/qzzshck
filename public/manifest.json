{
  "manifest_version": 3,
  "name": "Gray Style",
  "version": "0.1.0",
  "description": "Gray Style for Google Chrome",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "64": "icons/icon_64.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_title": "Gray Style",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://platonus.turan-edu.kz/*",
        "http://127.0.0.1:5500/*"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
